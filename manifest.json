{
  "name": "Netflix Chrome Extension",
  "version": "0.2",
  "description": "The decypher for netflix webrequest",
  "update_url": "http://www.stanford.edu/~huangty/netflix/updates.xml",
  "background_page" : "background.html",
  "page_action" :
  {
    "default_icon" : "netflix.png",
    "default_title" : "click to see the request log",
	"default_popup": "popup.html"
  },
  "permissions":  [
	"experimental",
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
